<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="add1" class="com.lara.Address" >
<property name="StreetName" value="saheb-para"/>
<property name="CityName" value="Asansol"/>
<property name="Pincode" value="723121"/>
</bean>


<bean id="add2" class="com.lara.Address" >
<property name="StreetName" value="32 main,2nd Cross,BTM 1st Stage"/>
<property name="CityName" value="Bangalore"/>
<property name="Pincode" value="560068"/>
</bean>
<bean id="p" class="com.lara.Person" >
<property name="FirstName" value="Manish"/>
<property name="LastName" value="Prasad"/>
<property name="Age" value="22"/>
<property name="Permanent_Address" ref="add1"/>
<property name="Present_Address" ref="add2"/>
</bean>
</beans>