<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
<bean id="a1" class="com.lara.Address">
<property name="House_No" value="121/B/12"/>
<property name="Street_Name" value="2nd Main"/>
</bean>
<bean id="p1" class="com.lara.Person" autowire="byType">
<property name="firstName" value="abc"/>
<property name="lastName" value="xyz"/>
<property name="age" value="23"/>
</bean>
</beans>